pipeline {
    agent { docker { image 'python:3.5.1' } }
    stages {
        stage('Build') {
            steps {
                sh 'echo "I am Iron Man!!"'
            }
        }
        stage('Update') {
            steps {
                sh 'yum update -y'
            }
        }
        stage('Decomission') {
            steps {
                sh 'echo "Would take the box down or something"'
            }
        }
    }
}
