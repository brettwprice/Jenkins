pipeline {
    agent { docker { image 'python:3.5.1' } }
    stages {
        stage('IronMan') {
            steps {
                sh 'echo "I am Iron Man!!"'
            }
        }
        stage('Thor') {
            steps {
                sh 'echo "God of Thunder"'
            }
        }
        stage('Guardians') {
            steps {
                sh 'echo "Whats your name?.... TAZERFACE!!! Hahahahahahahahahahahaha"'
            }
        }
        stage('Spiderman') {
            steps {
                sh 'echo "<Insert Whiny kid Here>!!"'
            }
        }
        stage('Ant Man') {
            steps {
                sh 'echo "He gets really big too.....?"'
            }
        }
    }
}
